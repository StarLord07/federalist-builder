{
  "name": "federalist-builder",
  "repository": {
    "type": "git",
    "url": "git://github.com/18f/federalist-builder.git"
  },
  "version": "0.0.0",
  "private": true,
  "description": "Checks an SQS queue and launches Federalist builds",
  "main": "app.js",
  "scripts": {
    "dev": "node -r .env app",
    "start": "node app",
    "test": "NODE_ENV=test mocha --require test/env.js --exit",
    "test:cover": "NODE_ENV=test nyc mocha --require test/env.js --exit",
    "eslint": "eslint ."
  },
  "author": "dhcole",
  "license": "CC0-1.0",
  "dependencies": {
    "@hapi/hapi": "^20.2.2",
    "aws-sdk": "^2.907.0",
    "axios": "^1.6.8",
    "bull": "^4.11.0",
    "cfenv": "^1.2.4",
    "jsonwebtoken": "^9.0.2",
    "newrelic": "^10.3.1",
    "winston": "^3.3.4"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "dotenv": "^8.2.0",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.23.0",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "mocha": "^8.4.0",
    "nock": "^13.0.11",
    "nyc": "^15.1.0",
    "sinon": "^10.0.0"
  },
  "engines": {
    "node": "14.x.x",
    "npm": "6.x.x"
  },
  "nyc": {
    "exclude": [
      "test/**/*.js"
    ],
    "reporter": [
      "lcov"
    ]
  }
